{
  "name": "@panter/promised",
  "description": "promise wrappers for functions",
  "author": "Claudio Romano <cro@panter.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panter/promised.git"
  },
  "homepage": "https://github.com/panter/promised#readme",
  "bugs": "https://github.com/panter/promised/issues",
  "keywords": [
    "promise",
    "es6",
    "wrapper"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --debug && npm run build:lib",
    "build:lib": "rimraf ./lib && babel src --out-dir lib",
    "lint": "eslint src/**.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "coveralls": "cat ./test/coverage/lcov.info | coveralls"
  },
  "main": "./dist/promised.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "6.x.x",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^3.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-loader": "^1.1.1",
    "ncp": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {

  }
}
